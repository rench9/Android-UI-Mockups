<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/white" />
            <stroke android:width="1dp" android:color="@color/primary_s1" />
            <corners android:radius="@dimen/radius2" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/primary_s2" />
            <stroke android:width="1dp" android:color="@color/primary_s2" />
            <corners android:radius="@dimen/radius2" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/primary_s3" />
            <stroke android:width="1dp" android:color="@color/primary_s3" />
            <corners android:radius="@dimen/radius2" />
        </shape>
    </item>
</selector>